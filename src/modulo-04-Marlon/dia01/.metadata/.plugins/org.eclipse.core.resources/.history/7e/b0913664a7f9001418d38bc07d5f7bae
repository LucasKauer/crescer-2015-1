package filmator;

public enum Genero {
	
	ACAO(0), ANIMACAO(1), AVENTURA(2), COMEDIA(3), CULT(4), DRAMA(5), FANTASIA(6),
	FAROESTE(7), FICCAO_CIENTIFICA(8), GUERRA(9), MUSICAL(10), POLICIAL(11), ROMANCE(12),
	SUSPENSE(13), TERROR(14);
	
	private int value;
	
	private Genero(int value) {
		this.value = value;
	}
}
