package filmator;

import static org.junit.Assert.*;

import org.junit.Test;

public class FilmatorTest {
	@Test
	public void atores() throws Exception {
		Ator actor1 = new Ator("Ator 1");
		Ator actor2 = new Ator("Ator 2");
		Ator actor3 = new Ator("Não deve retornar esse!");
		
		Filme movie = new Filme("Filme 1");
		
		movie.newActor(actor1);
		movie.newActor(actor2);
		movie.newActor(actor3);
		
		System.out.println(movie.actorsContainsStr("Ator"));
	}
}
